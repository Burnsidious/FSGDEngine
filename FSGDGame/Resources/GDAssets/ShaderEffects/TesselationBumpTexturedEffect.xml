<?xml version="1.0"?>
<ShaderEffect>
	<Technique name="Basic">
		<Pass name="P0">
			<VertexShader>LitTextured_VS.cso</VertexShader>
			<PixelShader>LitTextured_PS.cso</PixelShader>
		</Pass>	
	</Technique>
	<Technique name="GBuffersBump">
		<Pass name="P0">
			<VertexShader>GBuffersBumpTess_VS.cso</VertexShader>
			<HullShader>GBuffersBump_HS.cso</HullShader>
			<DomainShader>GBuffersBump_DS.cso</DomainShader>
			<PixelShader>GBuffersBump_PS.cso</PixelShader>
		</Pass>	
	</Technique>
	<Technique name="DepthCameraBump">
		<Pass name="P0">
			<VertexShader>DepthCamera_VS.cso</VertexShader>
			<PixelShader>Depth_PS.cso</PixelShader>
		</Pass>	
	</Technique>
	<Technique name="DepthSpotLightBump">
		<Pass name="P0">
			<VertexShader>DepthSpot_VS.cso</VertexShader>
			<PixelShader>Depth_PS.cso</PixelShader>
		</Pass>	
	</Technique>		
	<Technique name="DepthDirectionalLightBump">
		<Pass name="P0">
			<VertexShader>DepthDirectional_VS.cso</VertexShader>
			<PixelShader>Depth_PS.cso</PixelShader>
		</Pass>	
	</Technique>	
	<Technique name="DepthPointLightBump">
		<Pass name="P0">
			<VertexShader>DepthPoint_VS.cso</VertexShader>
			<GeometryShader>CubeMap_GS.cso</GeometryShader>
			<PixelShader>Depth_PS.cso</PixelShader>
		</Pass>	
	</Technique>
</ShaderEffect>
